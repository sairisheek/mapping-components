        <fullName>Status_Flag__c</fullName>
        <deprecated>false</deprecated>
        <description>Red, Yellow or Green flag for QuickBooks Online Invoices, representing Overdue, Due, and Paid</description>
        <externalId>false</externalId>
        <formula>IF(
  CONTAINS($User.UIThemeDisplayed, &apos;Theme4&apos;),

  IF( 
     ISBLANK( Internal_Status__c ),
     IF( OR(Status__c = &apos;Paid&apos;, Status__c = &apos;Closed&apos;),
	IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/flag_green_36.png&quot;, &quot;Paid&quot;),
	IF( Type__c = &apos;Credit Memo&apos;,
	   IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/flag_yellow_36.png&quot;, &quot;Unapplied&quot;),
	   IF( Amount_Overdue__c &gt; 0,
	     IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/flag_red_36.png&quot;, &quot;Overdue&quot;) ,
	     IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/flag_yellow_36.png&quot;, &quot;Open/Partial&quot; ) 
	   )
	)
     ),
     IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/void_deleted_36.png&quot;, &quot;Deleted / Voided&quot;)
  ),

  IF( 
     ISBLANK( Internal_Status__c ),
     IF( OR(Status__c = &apos;Paid&apos;, Status__c = &apos;Closed&apos;),
	IMAGE(&quot;/img/samples/flag_green.gif&quot;, &quot;Paid&quot;, 16, 16),
	IF( Type__c = &apos;Credit Memo&apos;,
	   IMAGE(&quot;/img/samples/flag_yellow.gif&quot;, &quot;Unapplied&quot;, 16, 16),
	   IF( Amount_Overdue__c &gt; 0,
	     IMAGE(&quot;/img/samples/flag_red.gif&quot;, &quot;Overdue&quot;, 16, 16) ,
	     IMAGE(&quot;/img/samples/flag_yellow.gif&quot;, &quot;Open/Partial&quot;, 16, 16) 
	   )
	)
      ),
      IMAGE(&quot;https://cdn.daddyanalytics.com/breadwinner/images/voided_deleted.png&quot;, &quot;Deleted / Voided&quot;)
  )
)</formula>
        <label>Status Flag</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Status__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>IF( 
  ISBLANK( Internal_Status__c ),
  CASE( 
    Type__c,
    
    &apos;Sales Receipt&apos;, &apos;Paid&apos;,
    &apos;Refund Receipt&apos;, &apos;Paid&apos;,

    &apos;Invoice&apos;, IF( Amount_Overdue__c &gt; 0,
                  &apos;Overdue&apos;,
                  IF( Amount_Due__c &gt; 0,
                    IF( Amount_Received__c &gt; 0,
                      &apos;Partial&apos;,
                      &apos;Open&apos;
                    ),
                    &apos;Paid&apos;
                  )
                ),

    IF( Credit_Memo_Unallocated_Credit__c &gt; 0,
      &apos;Unapplied&apos;,
      &apos;Closed&apos;
    )
  ),
  Internal_Status__c 
)</formula>
