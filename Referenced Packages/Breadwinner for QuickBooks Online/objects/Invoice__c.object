    <fields>
        <fullName>Invoice_URL__c</fullName>
        <deprecated>false</deprecated>
        <externalId>false</externalId>
        <formula>HYPERLINK( 
  &quot;https://&quot;&amp;IF( CONTAINS(QBO_Org_Name__c , &quot;Sandbox Company&quot;), &quot;sandbox.&quot; ,&quot;&quot;) &amp;&quot;qbo.intuit.com/app/&quot;&amp;
    CASE(
       Type__c,

       &apos;Credit Memo&apos;, &quot;creditmemo?txnId=&quot;&amp;RIGHT(Invoice_Id__c,(LEN(Invoice_Id__c)-FIND(&apos;-CM-&apos;, Invoice_Id__c) - 3 )),

       &apos;Sales Receipt&apos;, &quot;salesreceipt?txnId=&quot;&amp;RIGHT(Invoice_Id__c,(LEN(Invoice_Id__c)-FIND(&apos;-SR-&apos;, Invoice_Id__c)- 3)),

       &apos;Refund Receipt&apos;, &quot;refundreceipt?txnId=&quot;&amp;RIGHT(Invoice_Id__c,(LEN(Invoice_Id__c)-FIND(&apos;-RR-&apos;, Invoice_Id__c)- 3)),      
 
       &quot;invoice?txnId=&quot;&amp;RIGHT(Invoice_Id__c,(LEN(Invoice_Id__c)-FIND(&apos;-INV-&apos;, Invoice_Id__c) - 4 ))

       
    ),
  &quot;Click Here&quot;
)</formula>
